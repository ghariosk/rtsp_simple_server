{
    "name": "RTSP Simple Server Add-on",
    "version": "0.20.0",
    "slug": "rtsp_simple_server",
    "description": "Create RTSP server to broadcast streams, thanks to @aler9 - https://github.com/aler9/rtsp-simple-server",
    "arch": ["armv7", "aarch64", "amd64", "armhf"],
    "startup": "application",
    "boot": "auto",
    "ports": {
        "8554/tcp": 8554,
        "1935/tcp": 1935
    },
    "options": {
        "RTSP_PROTOCOLS": "tcp",
        "RTSP_PATHS_ALL_RUNONDEMAND": "echo 'incoming'",
        "RTSP_PATHS_ALL_RUNONDEMANDRESTART": "yes"
    },
    "schema": {
        "RTSP_PROTOCOLS": "str",
        "RTSP_PATHS_ALL_RUNONDEMAND": "str",
        "RTSP_PATHS_ALL_RUNONDEMANDRESTART": "str"
    },
    "image": "aler9/rtsp-simple-server",
    "ingress": false,
    "panel_icon": "mdi:cctv",
    "panel_title": "RTSP Server",
    "panel_admin": true,
    "gpio": false,
    "udev": false,
    "stage": "stable",
    "healthcheck": "http://localhost:8554/",
    "apparmor": true,
    "timeout": 10,
    "init": true,
    "advanced": false,
    "auto_update": true
}
